{
  "manifest_version": 3,
  "name": "HD FUTURETECH Proxy Manager",
  "version": "1.0.0",
  "description": "Quản lý proxy xoay và proxy tĩnh cho trình duyệt của bạn",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"]
}
